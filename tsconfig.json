{
  "compilerOptions": {
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom"
    ],
    "experimentalDecorators": true,
    "allowJs": true,
    "checkJs": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": false,
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*",
        "./client/*"
      ],
      "@/*": [
        "./*",
        "./client/*"
      ]
    },
    "types": [
      "@types/node",
      "@nuxt/vue-app",
      "@types/webpack-env",
    ]
  },
  // "include": [
  //   "-stub-/client/**/*.vue",
  //   "client/**/*.ts",
  //   "-stub-/tests/**/*.ts",
  //   "index.d.ts"
  // ],
  "exclude": ["node_modules", ".nuxt", "dist", "server"]
}
